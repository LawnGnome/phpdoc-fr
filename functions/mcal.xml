      </paramdef>
      <paramdef>int
       <parameter><optional>begin_day</optional></parameter>
      </paramdef>
      <paramdef>int
       <parameter><optional>end_year</optional></parameter>
      </paramdef>
      <paramdef>int
       <parameter><optional>end_month</optional></parameter>
      </paramdef>
      <paramdef>int
