<refentry id="function.apc-compile-file">
 <refnamediv>
  <refname>apc_compile_file</refname>
  <refpurpose>Stocke un fichier dans le cache, en évitant tous les filtres</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>apc_compile_file</methodname>
   <methodparam><type>string</type><parameter>filename</parameter></methodparam>
  </methodsynopsis>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>filename</parameter></term>
     <listitem>
      <para>
       Chemin complet ou relatif vers un fichier &php; qui sera compilé et stocké
       dans le cache.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

</refentry>

