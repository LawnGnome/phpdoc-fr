<refentry id="function.px-delete">
 <refnamediv>
  <refname>px_delete</refname>
  <refpurpose>Efface la ressource d'une base de données Paradox</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>px_delete</methodname>
   <methodparam><type>resource</type><parameter>pxdoc</parameter></methodparam>
  </methodsynopsis>
  <para>
   Efface la ressource d'une base de données Paradox et libère toute la mémoire.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>

