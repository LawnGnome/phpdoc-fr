<!-- splitted from ./fr/functions/imap.xml, last change in rev 1.15 -->
<!-- last change to 'imap-unsubscribe' in en/ tree in rev 1.9 -->
 <refentry id="function.imap-unsubscribe">
  <refnamediv>
   <refname>imap_unsubscribe</refname>
   <refpurpose>Termine la souscription &agrave; une bo&icirc;te aux lettres</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
     <methodsynopsis>
      <type>bool</type><methodname>imap_unsubscribe</methodname>
      <methodparam><type>string</type><parameter>imap_stream</parameter></methodparam>
      <methodparam><type>string</type><parameter>mbox</parameter></methodparam>
     </methodsynopsis>
   <para>
    <function>imap_unsubscribe</function> termine la souscription &agrave;
    la bo&icirc;te aux lettres <parameter>mbox</parameter>.
   </para>
   <para>
    <function>imap_unsubscribe</function> retourne &true;
    en cas de succ&egrave;s, et &false; en cas d'erreur.
   </para>
  </refsect1>
 </refentry>

