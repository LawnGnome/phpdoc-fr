<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./fr/functions/imap.xml, last change in rev 1.15 -->
<!-- last change to 'imap-msgno' in en/ tree in rev 1.2 -->
 <refentry id="function.imap-msgno">
  <refnamediv>
   <refname>imap_msgno</refname>
   <refpurpose>
    Retourne le numéro de séquence de message pour un UID donné.
   </refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>int</type><methodname>imap_msgno</methodname>
     <methodparam><type>resource</type><parameter>imap_stream</parameter></methodparam>
     <methodparam><type>int</type><parameter>uid</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>imap_msgno</function> retourne le numéro de séquence
    de message pour l'UID <parameter>uid</parameter>.
    C'est la fonction contraire de <function>imap_uid</function>.
   </para>
  </refsect1>
 </refentry>

