<!-- splitted from ./fr/functions/imap.xml, last change in rev 1.15 -->
<!-- last change to 'imap-qprint' in en/ tree in rev 1.2 -->
 <refentry id="function.imap-qprint">
  <refnamediv>
   <refname>imap_qprint</refname>
   <refpurpose>
    Convertit une cha&icirc;ne &agrave; guillemets en une
    cha&icirc;ne &agrave; 8 bits.
   </refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>string</type><methodname>imap_qprint</methodname>
     <methodparam><type>string</type><parameter>string</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>imap_qprint</function> convertit la cha&icirc;ne &agrave;
    guillemets <parameter>string</parameter> en une cha&icirc;ne &agrave;
    8 bits.
   </para>
   <para>
    <function>imap_qprint</function> retourne une cha&icirc;ne 8 bits (binaire).
   </para>
   <para>
    Voir aussi
    <function>imap_8bit</function>.
   </para>
  </refsect1>
 </refentry>

