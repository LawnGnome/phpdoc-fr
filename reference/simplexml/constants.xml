<section id="simplexml.constants">
 &reftitle.constants;
 &extension.constants;
 <variablelist>
  <varlistentry>
   <term>
    <constant>LIBXML_NOENT</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Substitue les entités
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_DTDLOAD</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Charge un sous-ensemble externe
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_DTDATTR</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Attributs par défaut de la DTD
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_DTDVALID</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Valide avec la DTD
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_NOERROR</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Supprime les messages d'erreurs
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_NOWARNING</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Supprime les alertes.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_NOBLANKS</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Supprime les noeuds vides
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_XINCLUDE</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Implémente les substitutions XInclude
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_NSCLEAN</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Supprime les déclaratons redondantes d'espaces de noms
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_NOCDATA</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Fusionne les blocks CDATA en noeuds de texte
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>LIBXML_NONET</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Désactive l'accès au réseau durant le chargement de documents
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
 <note>
  <para>
   Ces constantes ont été ajoutées en &php; 5.1.0.
  </para>
 </note>
</section>

