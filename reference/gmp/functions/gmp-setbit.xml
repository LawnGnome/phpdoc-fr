<!-- splitted from ./fr/functions/gmp.xml, last change in rev 1.10 -->
<!-- last change to 'gmp-setbit' in en/ tree in rev 1.1 -->
 <refentry id="function.gmp-setbit">
  <refnamediv>
   <refname>gmp_setbit</refname>
   <refpurpose>Modifie un bit</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>resource</type><methodname>gmp_setbit</methodname>
     <methodparam><type>resource</type><parameter>&amp;a</parameter></methodparam>
     <methodparam><type>int</type><parameter>index</parameter></methodparam>
     <methodparam choice="opt"><type>boolean</type><parameter>set_clear</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>gmp_setbit</function> modifie le bit <parameter>index</parameter>
    dans <parameter>a</parameter>. <parameter>set_clear</parameter> indique si
    le bit est mis &agrave; 0 ou 1. Par d&eacute;faut, il est mis &agrave; 1.
   </para>
  </refsect1>
 </refentry>

