<!-- splitted from ./fr/functions/gmp.xml, last change in rev 1.10 -->
<!-- last change to 'gmp-random' in en/ tree in rev 1.1 -->
 <refentry id="function.gmp-random">
  <refnamediv>
   <refname>gmp_random</refname>
   <refpurpose>Nombre GMP aléatoire</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>resource</type><methodname>gmp_random</methodname>
     <methodparam><type>int</type><parameter>limiter</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>gmp_random</function> génère un nombre aléatoire.
    Ce nombre est limité par <parameter>limiter</parameter>. Si
    <parameter>limiter</parameter> est négatif, un nombre négatif est
    généré.
   </para>
  </refsect1>
 </refentry>

