 <refentry id="function.gmp-mul">
  <refnamediv>
   <refname>gmp_mul</refname>
   <refpurpose>Multiplication de 2 nombres GMP</refpurpose>
  </refnamediv>
  <refsect1>
   &reftitle.description;
    <methodsynopsis>
     <type>resource</type><methodname>gmp_mul</methodname>
     <methodparam><type>resource</type><parameter>a</parameter></methodparam>
     <methodparam><type>resource</type><parameter>b</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>gmp_mul</function> multiplie les nombres GMP <parameter>a</parameter>
    et <parameter>b</parameter>.
   </para>
   <example>
    <title>Exemple avec <function>gmp_mul</function></title>
    <programlisting role="php">
<![CDATA[
<?php
$mul = gmp_mul("12345678", "2000");
echo gmp_strval($mul) . "\n";
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
24691356000
]]>
    </screen>
   </example>
  </refsect1>
 </refentry>

