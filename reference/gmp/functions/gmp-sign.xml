<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./fr/functions/gmp.xml, last change in rev 1.10 -->
<!-- last change to 'gmp-sign' in en/ tree in rev 1.1 -->
 <refentry id="function.gmp-sign">
  <refnamediv>
   <refname>gmp_sign</refname>
   <refpurpose>Signe du nombre GMP</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>int</type><methodname>gmp_sign</methodname>
     <methodparam><type>resource</type><parameter>a</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>gmp_sign</function> retourne le signe de <parameter>a</parameter> :
    1 si <parameter>a</parameter> est positif et -1 s'il est n&eacute;gatif.
   </para>
  </refsect1>
 </refentry>

