 <refentry id="function.gmp-popcount">
  <refnamediv>
   <refname>gmp_popcount</refname>
   <refpurpose>Compte de population</refpurpose>
  </refnamediv>
  <refsect1>
   &reftitle.description;
    <methodsynopsis>
     <type>int</type><methodname>gmp_popcount</methodname>
     <methodparam><type>resource</type><parameter>a</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>gmp_popcount</function> dénombre la population de
    <parameter>a</parameter>.
   </para>
   <example>
    <title>Exemple avec <function>gmp_popcount</function></title>
    <programlisting role="php">
<![CDATA[
<?php
$pop1 = gmp_init("10000101", 2); // 3 1's
echo gmp_popcount($pop1) . "\n";
$pop2 = gmp_init("11111110", 2); // 7 1's
echo gmp_popcount($pop2) . "\n";
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
3
7
]]>
    </screen>
   </example>
  </refsect1>
 </refentry>

