 <refnamediv>
  <refname>Imagick::getInterlaceScheme</refname>
  <refpurpose>Lit le schéma d'entrelacement de l'objet</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>Imagick::getInterlaceScheme</methodname>
   <void/>
  </methodsynopsis>
  &warn.undocumented.func;
  <para>
