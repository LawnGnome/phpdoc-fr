<!-- splitted from ./fr/functions/filepro.xml, last change in rev 1.7 -->
<!-- last change to 'filepro-fieldcount' in en/ tree in rev 1.2 -->
 <refentry id="function.filepro-fieldcount">
  <refnamediv>
   <refname>filepro_fieldcount</refname>
   <refpurpose>Retourne le nombre de champs dans une base filePro</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>int</type><methodname>filepro_fieldcount</methodname>
     <void/>
    </methodsynopsis>
   <para>
    <function>filepro_fieldcount</function> retourne le nombre de
    champs (ou colonnes) d'une base filePro.
   </para>
   <para>
    Voir aussi 
    <function>filepro</function>.
   </para>
  </refsect1>
 </refentry>

