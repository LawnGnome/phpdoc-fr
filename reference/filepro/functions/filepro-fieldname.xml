<!-- splitted from ./fr/functions/filepro.xml, last change in rev 1.7 -->
<!-- last change to 'filepro-fieldname' in en/ tree in rev 1.2 -->
 <refentry id="function.filepro-fieldname">
  <refnamediv>
   <refname>filepro_fieldname</refname>
   <refpurpose>Retourne le nom d'un champ Filepro</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>string</type><methodname>filepro_fieldname</methodname>
     <methodparam><type>int</type><parameter>field_number</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>filepro_fieldname</function> retourne le nom du champ
    d'index <parameter>field_number</parameter>.
   </para>
  </refsect1>
 </refentry>

