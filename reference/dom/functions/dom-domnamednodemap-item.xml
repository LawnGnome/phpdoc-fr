  <refentry id='function.dom-domnamednodemap-item'>
   <refnamediv>
    <refname>DOMNamedNodeMap->item</refname>
    <refpurpose>Retourne un noeud selon son index</refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
    <classsynopsis>
     <ooclass><classname>DOMNamedNodeMap</classname></ooclass>
     <methodsynopsis>
      <type>DOMNode</type>
      <methodname>item</methodname>
      <methodparam><type>int</type><parameter>index</parameter></methodparam>
     </methodsynopsis>
    </classsynopsis>
    <para>
     Cette fonction retourne le noeud d'index <parameter>index</parameter>
     dans l'objet DOMNamedNodeMap.
    </para>
   </refsect1>
  </refentry>

