<refentry xml:id="function.swf-nextid" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>swf_nextid</refname>
  <refpurpose>Retourne le prochain identifiant d'objet libre</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>swf_nextid</methodname>
   <void/>
  </methodsynopsis>
  <para>
   <function>swf_nextid</function> retourne le prochain identifiant d'objet libre.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne l'identifiant, sous la forme d'un &integer;.
  </para>
 </refsect1>
</refentry>

