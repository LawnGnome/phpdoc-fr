  <refentry id="function.swf-actiongeturl">
   <refnamediv>
    <refname>swf_actiongeturl</refname>
    <refpurpose>Retourne l'URL d'une animation Shockwave Flash</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>swf_actiongeturl</methodname>
      <methodparam><type>string</type><parameter>url</parameter></methodparam>
      <methodparam><type>string</type><parameter>target</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>swf_actiongeturl</function> lit l'URL <parameter>url</parameter>,
     avec la destination <parameter>target</parameter>.
    </para>
   </refsect1>
  </refentry>

