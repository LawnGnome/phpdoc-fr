<refentry xml:id="cairoimagesurface.getheight" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>CairoImageSurface::getHeight</refname>
  <refpurpose>Récupère la hauteur d'un CairoImageSurface</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>int</type><methodname>CairoImageSurface::getHeight</methodname>
   <void />
  </methodsynopsis>
  <para>
   Récupère la hauteur d'un <classname>CairoImageSurface</classname>.
  </para>

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   La hauteur d'un objet CairoImageSurface.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <methodname>CairoImageSurface::getHeight</methodname></title>
    <programlisting role="php">
<![CDATA[
<?php
// Création d'une nouvelle surface pour l'image
$surface = new CairoImageSurface(CairoFormat::ARGB32, 80, 50);

// Récupère la hauteur
var_dump($surface->getHeight());

?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
int(50)
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>CairoImageSurface::getWidth</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

