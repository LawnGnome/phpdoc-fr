<!-- splitted from ./fr/functions/ldap.xml, last change in rev 1.10 -->
<!-- last change to 'ldap-unbind' in en/ tree in rev 1.2 -->
 <refentry id="function.ldap-unbind">
  <refnamediv>
   <refname>ldap_unbind</refname>
   <refpurpose>Termine la liaison avec un serveur LDAP.</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>int</type><methodname>ldap_unbind</methodname>
     <methodparam><type>resource</type><parameter>link_identifier</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>ldap_unbind</function> retourne &true;
    en cas de succ&egrave;s, et &true; sinon.
   </para>
   <para>
    <function>ldap_unbind</function> termine la liaison avec le serveur LDAP.
   </para>
  </refsect1>
 </refentry>

