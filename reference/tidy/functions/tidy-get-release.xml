  <refentry id="function.tidy-get-release">
   <refnamediv>
    <refname>tidy_get_release</refname>
    <refpurpose>
     Retourne la date de publication (version) de la librairie Tidy
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
    &title.procedural;
    <methodsynopsis>
     <type>string</type><methodname>tidy_get_release</methodname>
     <void/>
    </methodsynopsis>
    &title.oop;
    <methodsynopsis>
     <type>string</type><methodname>tidy->getRelease</methodname>
     <void />
    </methodsynopsis>
    <para>
     <function>tidy_get_release</function> retourne une chaîne avec
     la date de publication de la librairie tidy.
    </para>
   </refsect1>
  </refentry>

