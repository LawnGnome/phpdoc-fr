   </listitem>
  </varlistentry>
  <varlistentry xml:id="ziparchive.constants.fl-unchanged">
   <term>
    <constant>ZipArchive::FL_UNCHANGED</constant>
     (&integer;)
   </term>
   <listitem>
    <simpara>
     Utilise les données originales, en ignorant les modifications.
    </simpara>
   </listitem>
  </varlistentry>

