  <refentry id="function.xslt-backend-name">
   <refnamediv>
    <refname>xslt_backend_name</refname>
    <refpurpose>
     Retourne le nom du backend
    </refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
    <methodsynopsis>
     <type>string</type><methodname>xslt_backend_name</methodname>
     <void/>
    </methodsynopsis>
    <para>
     <function>xslt_backend_name</function> retourne toujours la chaîne
     <productname>Sablotron</productname>.
    </para>
   </refsect1>
   <refsect1>
    &reftitle.examples;
     <example>
      <title>Exemple avec <function>xslt_backend_name</function></title>
      <programlisting role="php">
<![CDATA[
<?php

echo xslt_backend_name(); // Sablotron

?>
]]>
      </programlisting>
     </example>
   </refsect1>
   <refsect1>
    &reftitle.seealso;
    <para>
     <function>xslt_backend_info</function>&listendand;
     <function>xslt_backend_version</function>.
    </para>
   </refsect1>
  </refentry>

