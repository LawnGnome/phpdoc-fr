<refentry xml:id="function.pdf-set-parameter" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>PDF_set_parameter</refname>
  <refpurpose>Modifie certains paramètres</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>PDF_set_parameter</methodname>
   <methodparam><type>resource</type><parameter>p</parameter></methodparam>
   <methodparam><type>string</type><parameter>key</parameter></methodparam>
   <methodparam><type>string</type><parameter>value</parameter></methodparam>
  </methodsynopsis>
  <para>
   <function>PDF_set_parameter</function> modifie les paramètres PDFlib
   de type texte.
   &return.success;
  </para>
 </refsect1>
</refentry>

