<!-- last change to 'pdf-set-char-spacing' in en/ tree in rev 1.42 -->
  <refentry id="function.pdf-set-char-spacing">
   <refnamediv>
    <refname>pdf_set_char_spacing</refname>
    <refpurpose>Fixe l'espacement des caract&egrave;res.</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>pdf_set_char_spacing</methodname>
      <methodparam><type>resource</type><parameter>pdf_object</parameter></methodparam>
      <methodparam><type>double</type><parameter>space</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>pdf_set_char_spacing</function> modifie l'espacement
     des caract&egrave;res.
    </para>
    <para>
     Voir aussi <function>pdf_set_word_spacing</function>
     et <function>pdf_set_leading</function>.
    </para>
   </refsect1>
  </refentry>

