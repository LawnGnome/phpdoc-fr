  <refentry id="function.pdf-get-pdi-value">
   <refnamediv>
    <refname>pdf_get_pdi_value</refname>
    <refpurpose>Lit des param&egrave;tres num&eacute;riques dans le document PDF d'entr&eacute;e</refpurpose>
