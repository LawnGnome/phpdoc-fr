  <refentry id="function.pdf-stroke">
   <refnamediv>
    <refname>pdf_stroke</refname>
    <refpurpose>Dessine la ligne le long du chemin</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>bool</type><methodname>pdf_stroke</methodname>
      <methodparam><type>resource</type><parameter>pdfdoc</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>pdf_stroke</function> dessine la ligne le long du chemin courant,
     avec la couleur courante et la largeur de ligne courante, puis
     clôt le chemin. 
     &return.success;
    </para>
   </refsect1>
  </refentry>

