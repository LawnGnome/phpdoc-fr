<refentry xml:id="function.apd-dump-persistent-resources" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>apd_dump_persistent_resources</refname>
  <refpurpose>Retourne toutes les ressources persistantes dans un tableau</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>apd_dump_persistent_resources</methodname>
   <void/>
  </methodsynopsis>
  <para>
   <function>apd_dump_persistent_resources</function> retourne toutes
   les ressources persistantes dans un tableau.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Un tableau contenant toutes les ressources persistantes.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>apd_dump_persistent_resources</function></title>
    <programlisting role="php">
<![CDATA[
<?php
print_r(apd_dump_persistent_resources());
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>apd_dump_regular</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

