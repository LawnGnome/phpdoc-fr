<refentry xml:id="function.harupage-getcmykfill" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HaruPage::getCMYKFill</refname>
  <refpurpose>Récupère la couleur de remplissage courante</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>array</type><methodname>HaruPage::getCMYKFill</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Récupère la couleur de remplissage courante.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne la couleur de remplissage courante, sous la forme d'un
   tableau de quatre éléments ("c", "m", "y" et "k").
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  &haru.error;
 </refsect1>

</refentry>

