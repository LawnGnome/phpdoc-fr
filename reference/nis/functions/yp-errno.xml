<refentry xml:id="function.yp-errno" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>yp_errno</refname>
  <refpurpose>Retourne le code d'erreur de la dernière opération NIS</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>yp_errno</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Retourne le code d'erreur de la dernière opération NIS.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne une des constantes <literal>YPERR_XXX</literal>.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>yp_err_string</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

