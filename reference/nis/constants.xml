<appendix xml:id="nis.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <variablelist>
  <varlistentry xml:id="constant.yperr-access">
   <term>
    <constant>YPERR_ACCESS</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     violation d'accès (ajouté récemment et uniquement disponible depuis PECL SVN actuellement)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-badargs">
   <term>
    <constant>YPERR_BADARGS</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Les arguments de la fonction sont incorrects.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-baddb">
   <term>
    <constant>YPERR_BADDB</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     La base de données YP est incorrecte.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-busy">
   <term>
    <constant>YPERR_BUSY</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     La base de données est occupée.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-domain">
   <term>
    <constant>YPERR_DOMAIN</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Impossible de se connecter au serveur sur ce domaine.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-key">
   <term>
    <constant>YPERR_KEY</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Clé introuvable dans cette carte.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-map">
   <term>
    <constant>YPERR_MAP</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Carte introuvable sur ce domaine.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-nodom">
   <term>
    <constant>YPERR_NODOM</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Le nom du domaine local n'est pas défini.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-nomore">
   <term>
    <constant>YPERR_NOMORE</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Plus d'enregistrements dans la base de données.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-pmap">
   <term>
    <constant>YPERR_PMAP</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Communication avec portmapper impossible.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-resrc">
   <term>
    <constant>YPERR_RESRC</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Échec dans l'allocation de la ressource.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-rpc">
   <term>
    <constant>YPERR_RPC</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Échec RPC - le domaine n'a pu être contacté
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-ypbind">
   <term>
    <constant>YPERR_YPBIND</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Communication impossible avec ypbind
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-yperr">
   <term>
    <constant>YPERR_YPERR</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Erreur yp interne ou du client.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-ypserv">
   <term>
    <constant>YPERR_YPSERV</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Communication impossible avec ypserv.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yperr-vers">
   <term>
    <constant>YPERR_VERS</constant>
    (&integer;)
   </term>
   <listitem>
    <simpara>
     Version yp incorrecte.
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
</appendix>

