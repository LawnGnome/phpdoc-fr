<!-- last change to 'usleep' in en/ tree in rev 1.14 -->
  <refentry id="function.usleep">
   <refnamediv>
    <refname>usleep</refname>
    <refpurpose>Retarde l'ex&eacute;cution en micro-secondes </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
     <type>void</type><methodname>usleep</methodname>
     <methodparam><type>int</type><parameter>micro_seconds</parameter></methodparam>
     </methodsynopsis>
    <simpara>
     <function>sleep</function> retarde l'ex&eacute;cution du programme pendant
     <parameter>micro_seconds</parameter> micro-secondes.
    </simpara>
    <simpara>
     Voir aussi
     <function>sleep</function>.
    </simpara>
    <note>
     <simpara>
      <function>sleep</function> est inop&eacute;rante sous Windows
     </simpara>
    </note>
   </refsect1>
  </refentry>

