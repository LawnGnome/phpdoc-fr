<appendix xml:id="net-gopher.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <table>
  <title>Constantes Net_Gopher</title>
  <tgroup cols="3">
   <thead>
    <row>
     <entry>Constante</entry>
     <entry>Valeur</entry>
     <entry>Description</entry>
    </row>
   </thead>
   <tbody>
    <row>
     <entry><constant>GOPHER_DOCUMENT</constant></entry>
     <entry>0</entry>
     <entry>Document standart <literal>text/plain</literal>.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_DIRECTORY</constant></entry>
     <entry>1</entry>
     <entry>Une ressource contenant une liste de dossiers formatée gopher.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_BINHEX</constant></entry>
     <entry>4</entry>
     <entry>Un fichier binaire encodé BinHex.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_DOSBINARY</constant></entry>
     <entry>5</entry>
     <entry>Une archive binaire formaté DOS.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_UUENCODED</constant></entry>
     <entry>6</entry>
     <entry>Un fichier UUEncoded.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_BINARY</constant></entry>
     <entry>9</entry>
     <entry>Un fichier binaire générique.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_INFO</constant></entry>
     <entry>255</entry>
     <entry>Une entrée informative.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_HTTP</constant></entry>
     <entry>254</entry>
     <entry>Une référence vers une ressource HTTP.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_UNKNOWN</constant></entry>
     <entry>-1</entry>
     <entry>
      Une entrée non reconnue.
     </entry>
    </row>
   </tbody>
  </tgroup>
 </table>
</appendix>

