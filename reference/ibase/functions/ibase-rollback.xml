<!-- splitted from ./fr/functions/ibase.xml, last change in rev 1.9 -->
<!-- last change to 'ibase-rollback' in en/ tree in rev 1.23 -->
 <refentry id="function.ibase-rollback">
  <refnamediv>
   <refname>ibase_rollback</refname>
   <refpurpose>Annule une transaction iBase</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>int</type><methodname>ibase_rollback</methodname>
     <methodparam choice="opt"><type>int</type><parameter>link_identifier</parameter></methodparam>
     <methodparam><type>int</type><parameter>trans_number</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>ibase_rollback</function> annule la transaction
    <parameter>trans_number</parameter> qui a été
    préparée avec <function>ibase_trans</function>.
    &return.success;
   </para>
  </refsect1>
 </refentry>

