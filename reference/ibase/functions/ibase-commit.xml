 <refentry id="function.ibase-commit">
  <refnamediv>
   <refname>ibase_commit</refname>
   <refpurpose>Valide une transaction iBase</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
     <methodsynopsis>
      <type>bool</type><methodname>ibase_commit</methodname>
      <methodparam choice="opt"><type>resource</type><parameter>link_identifier</parameter></methodparam>
      <methodparam><type>int</type><parameter>trans_number</parameter></methodparam>
     </methodsynopsis>
   <para>
    <function>ibase_commit</function> valide la transaction
    <parameter>trans_number</parameter>, qui a été
    préparée avec <function>ibase_trans</function>.
     &return.success;
   </para>
  </refsect1>
 </refentry>

