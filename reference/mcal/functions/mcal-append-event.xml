  <refentry xml:id="function.mcal-append-event" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>mcal_append_event</refname>
    <refpurpose>
      Enregistre un nouvel événement dans un calendrier MCAL
    </refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
     <methodsynopsis>
      <type>int</type><methodname>mcal_append_event</methodname>
      <methodparam><type>int</type><parameter>mcal_stream</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>mcal_append_event</function> enregistre
     l'événement global dans le calendrier
     MCAL <parameter>mcal_stream</parameter>.
    </para>
    <para>
     <function>mcal_append_event</function> retourne l'id de l'enregistrement
     ainsi inséré.
    </para>
   </refsect1>
  </refentry>

