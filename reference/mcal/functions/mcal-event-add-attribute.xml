<!-- last change to 'mcal-event-add-attribute' in en/ tree in rev 1.11 -->
  <refentry id="function.mcal-event-add-attribute">
   <refnamediv>
    <refname>mcal_event_add_attribute</refname>
    <refpurpose>
     Ajoute un attribut et une valeur à la structure globale
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>mcal_event_add_attribute</methodname>
      <methodparam><type>int</type><parameter>stream</parameter></methodparam>
      <methodparam><type>string</type><parameter>attribute</parameter></methodparam>
      <methodparam><type>string</type><parameter>value</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>mcal_event_add_attribute</function> ajoute l'attribut
     <parameter>attribute</parameter> à la structure globale,
     avec la valeur <parameter>value</parameter>.
    </para>
   </refsect1>
  </refentry>

