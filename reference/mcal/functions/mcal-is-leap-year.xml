  <refentry xml:id="function.mcal-is-leap-year" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>mcal_is_leap_year</refname>
    <refpurpose>
      Vérifie que l'année est bissextile
    </refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
     <methodsynopsis>
      <type>bool</type><methodname>mcal_is_leap_year</methodname>
      <methodparam><type>int</type><parameter>year</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>mcal_is_leap_year</function> retourne 1 si l'année
      <parameter>year</parameter> est bissextile, et 0 sinon.
    </para>
   </refsect1>
  </refentry>

