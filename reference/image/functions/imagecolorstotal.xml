<!-- splitted from ./fr/functions/image.xml, last change in rev 1.24 -->
<!-- last change to 'imagecolorstotal' in en/ tree in rev 1.36 -->
 <refentry id="function.imagecolorstotal">
  <refnamediv>
   <refname>ImageColorsTotal</refname>
   <refpurpose>Calcule le nombre de couleurs d'une palette.</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>int</type><methodname>imagecolorstotal</methodname>
     <methodparam><type>resource</type><parameter>im</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>imagecolorstotal</function> retourne le nombre de couleurs
    de la palette.
   </para>
   <para>
    Voir aussi
    <function>imagecolorat</function> et
    <function>imagecolorsforindex</function>.
   </para>
  </refsect1>
 </refentry>

