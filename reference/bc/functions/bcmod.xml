 <refentry id="function.bcmod">
  <refnamediv>
   <refname>bcmod</refname>
   <refpurpose>
    Retourne le reste d'une division entre nombres de grande taille
   </refpurpose>
  </refnamediv>
  <refsect1>
   &reftitle.description;
    <methodsynopsis>
     <type>string</type><methodname>bcmod</methodname>
     <methodparam><type>string</type><parameter>left_operand</parameter></methodparam>
     <methodparam><type>string</type><parameter>modulus</parameter></methodparam>
    </methodsynopsis>
    <para>
     <function>bcmod</function> retourne le reste de la division entre
     <parameter>left_operand</parameter> en utilisant <parameter>modulus</parameter>.
    </para>
   </refsect1>
   <refsect1>
    &reftitle.examples;
    <example>
     <title>Exemple avec <function>bcmod</function></title>
     <programlisting role="php">
<![CDATA[
<?php
echo bcmod(4, 2); // 0
echo bcmod(2, 4); // 2
?>
]]>
     </programlisting>
    </example>
   </refsect1>
   <refsect1>
    &reftitle.seealso;
    <para>
     <function>bcdiv</function>.
    </para>
   </refsect1>
  </refentry>

