<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.openssl-pkcs12-read">
 <refnamediv>
  <refname>openssl_pkcs12_read</refname>
  <refpurpose>Lit un certificat PKCS#12 dans un tableau</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>openssl_pkcs12_read</methodname>
   <methodparam><type>string</type><parameter>pkcs12</parameter></methodparam>
   <methodparam><type>array</type><parameter role="reference">certs</parameter></methodparam>
   <methodparam><type>string</type><parameter>pass</parameter></methodparam>
  </methodsynopsis>
  <para>
   <function>openssl_pkcs12_read</function> lit le certificat PKCS#12 fourni par le
   paramètre <parameter>pkcs12</parameter> dans un tableau nommé <parameter>certs</parameter>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>pkcs12</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>certs</parameter></term>
     <listitem>
      <para>
       En cas de succès, ce tableau contiendra les données du certificat.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>pass</parameter></term>
     <listitem>
      <para>
       Mot de passe de chiffrement pour déverrouiller le fichier PKCS#12.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

</refentry>

