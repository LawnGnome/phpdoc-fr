<refentry xml:id="function.httpresponse-capture" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpResponse::capture</refname>
  <refpurpose>Capture la sortie du script</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>static</modifier>
   <type>void</type><methodname>HttpResponse::capture</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Capture la sortie du script.
  </para>

 </refsect1>
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Exemple avec <function>HttpResponse::capture</function></title>
    <programlisting role="php">
<![CDATA[
<?php
HttpResponse::setCache(true);
HttpResponse::capture();
// script follows
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>HttpResponse::send</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

