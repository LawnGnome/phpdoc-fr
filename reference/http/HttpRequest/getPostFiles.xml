<refentry xml:id="function.HttpRequest-getPostFiles" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpRequest::getPostFiles</refname>
  <refpurpose>Récupère les fichiers POST</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>array</type><methodname>HttpRequest::getPostFiles</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Récupère tous les fichiers POST précédemment ajoutés.
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne un tableau contenant les fichiers POST actuellement définis.
  </para>
 </refsect1>
</refentry>

