<refentry xml:id="httprequest.getquerydata" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpRequest::getQueryData</refname>
  <refpurpose>Récupère les données de la requête</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>string</type><methodname>HttpRequest::getQueryData</methodname>
   <void/>
  </methodsynopsis>

  <para>
   Récupère les données de la requête courante sous la forme d'une chaîne encodée URL.
  </para>

 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne une chaîne contenant la requête encodée URL.
  </para>
 </refsect1>
</refentry>

