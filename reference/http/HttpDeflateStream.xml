<refentry xml:id="http.HttpDeflateStream" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>HttpDeflateStream</refname>
  <refpurpose>Classe de compression du flux HTTP</refpurpose>
 </refnamediv>
 <refsynopsisdiv>
  <synopsis>classe HttpDeflateStream</synopsis>
 </refsynopsisdiv>
 <refsect1 role="members" xml:id="http.HttpDeflateStream.members">
  <title>Membres de la classe</title>
  <refsect2 xml:id="http.HttpDeflateStream.constants">
   &reftitle.constants;
   <table>
    <title/>
    <tgroup cols="3">
     <thead>
      <row>
       <entry>Type</entry>
       <entry>Nom</entry>
       <entry>Description</entry>
      </row>
     </thead>
     <tbody>
      <row>
       <entry>int</entry>
       <entry>TYPE_GZIP</entry>
       <entry>encodage gzip</entry>
      </row>
      <row>
       <entry>int</entry>
       <entry>TYPE_ZLIB</entry>
       <entry>zlib AKA encodage compressé</entry>
      </row>
      <row>
       <entry>int</entry>
       <entry>TYPE_RAW</entry>
       <entry>encodage compressé</entry>
      </row>
      <row>
       <entry>int</entry>
       <entry>LEVEL_DEF</entry>
       <entry>degré de compression par défaut</entry>
      </row>
      <row>
       <entry>int</entry>
       <entry>LEVEL_MIN</entry>
       <entry>degré de compression minimum</entry>
      </row>
      <row>
       <entry>int</entry>
       <entry>LEVEL_MAX</entry>
       <entry>degré de compression maximum</entry>
      </row>
      <row>
       <entry>int</entry>
       <entry>STRATEGY_DEF</entry>
       <entry>stratégie par défaut</entry>
