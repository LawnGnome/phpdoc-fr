  <refentry xml:id="function.hwapi-userlist" xmlns="http://docbook.org/ns/docbook">
   <refnamediv>
    <refname>hw_api->userlist</refname>
    <refpurpose>Retourne la liste des utilisateurs connectés</refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
    <methodsynopsis>
     <type>array</type><methodname>hw_api->userlist</methodname>
     <methodparam><type>array</type><parameter>parameter</parameter></methodparam>
    </methodsynopsis>
    <para>
     <function>hw_api->userlist</function> retourne la liste des 
     utilisateurs connectés.
    </para>
    <para>
     Voir aussi
     <function>hwapi_user</function>.
    </para>
   </refsect1>
  </refentry>

