  <refentry id="function.variant-neg">
   <refnamediv>
    <refname>variant_neg</refname>
    <refpurpose>
     Effctue une négation logique sur un variant
    </refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
    <methodsynopsis>
     <type>mixed</type><methodname>variant_neg</methodname>
     <methodparam><type>mixed</type><parameter>variant</parameter></methodparam>
    </methodsynopsis>
    <para>
     Effectue une négation logique de <parameter>variant</parameter> et 
     retourne le résultat.
    </para>
    &reference.com.variant-arith;
   </refsect1>
  </refentry>

