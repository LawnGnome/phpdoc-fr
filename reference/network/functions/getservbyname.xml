<!-- splitted from ./fr/functions/network.xml, last change in rev 1.4 -->
<!-- last change to 'getservbyname' in en/ tree in rev 1.2 -->
  <refentry id="function.getservbyname">
   <refnamediv>
    <refname>getservbyname</refname>
    <refpurpose>
      Retourne le num&eacute;ro de port associ&eacute; &agrave; un service
      Internet et un protocole.
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>int</type><methodname>getservbyname</methodname>
      <methodparam><type>string</type><parameter>service</parameter></methodparam>
      <methodparam><type>string</type><parameter>protocol</parameter></methodparam>
     </methodsynopsis>
    <para>
      <function>getservbyname</function> retourne le num&eacute;ro de
      port associ&eacute; au service <parameter>service</parameter>
      et au protocole <parameter>protocol</parameter>, comme dans
      <filename>/etc/services</filename>. <parameter>protocol</parameter>
      vaut soit <literal>tcp</literal>, soit <literal>udp</literal>.
    </para>
    <para>
      Voir aussi
      <function>getservbyport</function>.
    </para>
   </refsect1>
  </refentry>

