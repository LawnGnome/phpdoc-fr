<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./fr/functions/network.xml, last change in rev 1.4 -->
<!-- last change to 'long2ip' in en/ tree in rev 1.18 -->
<refentry id="function.long2ip">
   <refnamediv>
    <refname>long2ip</refname>
    <refpurpose>
     Convertit une adresse IP (IPv4) en adresse IP numérique
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>string</type><methodname>long2ip</methodname>
      <methodparam><type>int</type><parameter>proper_address</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>long2ip</function> génère une adresse IP
     (format aaa.bbb.ccc.ddd) à partir de sa représentation
     littérale.
    </para>
    <para>
     Voir aussi
     <function>ip2long</function>
    </para>
   </refsect1>
  </refentry>

