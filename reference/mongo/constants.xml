<appendix xml:id="mongo.constants" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 &reftitle.constants;
 &no.constants;

  <variablelist>
  <varlistentry>
   <term><constant>Mongo::DEFAULT_HOST</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     &quot;localhost&quot;
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>Mongo::DEFAULT_PORT</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     27017
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoDB::PROFILING_OFF</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Désactive le profilage de base de données
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoDB::PROFILING_SLOW</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Désactive le profilage de base de données pour toutes les opérations
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoDB::PROFILING_ON</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Active le profilage de base de données pour toutes les opérations
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::LT</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     Moins que
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::LTE</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     Moins ou égal à
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::GT</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     Plus que
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::GTE</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     Plus ou égal à
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::IN</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     Dans
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::NE</constant>
    (<type>string</type>)
   </term>
   <listitem>
    <simpara>
     Non-égal
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::ASC</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Ascendant
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::DESC</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Descendant
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_ARRAY</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Type binaire par défaut : un tableau d'octets
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_UUID</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Identifiant universel unique
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_MD5</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Type MD5
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term><constant>MongoUtil::BIN_CUSTOM</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Type binaire défini par l'utilisateur
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>

</appendix>

