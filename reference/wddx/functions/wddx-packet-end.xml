  <refentry id="function.wddx-packet-end">
   <refnamediv>
    <refname>wddx_packet_end</refname>
    <refpurpose>Clôt un paquet WDDX</refpurpose>
   </refnamediv>
   <refsect1>
    &reftitle.description;
     <methodsynopsis>
      <type>string</type><methodname>wddx_packet_end</methodname>
      <methodparam><type>resource</type><parameter>packet_id</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>wddx_packet_end</function> clôt un paquet WDDX repéré
     par son identifiant <parameter>packet_id</parameter>.
    </para>
   </refsect1>
  </refentry>

