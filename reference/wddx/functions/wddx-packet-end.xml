<refentry xml:id="function.wddx-packet-end" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>wddx_packet_end</refname>
  <refpurpose>Clôt un paquet WDDX</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>wddx_packet_end</methodname>
   <methodparam><type>resource</type><parameter>packet_id</parameter></methodparam>
  </methodsynopsis>
  <para>
   Clôt un paquet WDDX.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>packet_id</parameter></term>
     <listitem>
      <para>
       Un paquet WDDX, retourné par la fonction
       <function>wddx_packet_start</function>.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Retourne une &string; contenant le paquet WDDX.
  </para>
 </refsect1>
</refentry>

