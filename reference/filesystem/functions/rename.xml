<?xml version="1.0" encoding="iso-8859-1"?>
<!-- splitted from ./fr/functions/filesystem.xml, last change in rev 1.27 -->
<!-- last change to 'rename' in en/ tree in rev 1.2 -->
 <refentry id="function.rename">
  <refnamediv>
   <refname>rename</refname>
   <refpurpose>Renomme un fichier.</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>int</type><methodname>rename</methodname>
     <methodparam><type>string</type><parameter>oldname</parameter></methodparam>
     <methodparam><type>string</type><parameter>newname</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>rename</function> rente de renommer le fichier
    <parameter>oldname</parameter> en <parameter>newname</parameter>.
   </para>
   <para>
    <function>rename</function> retourne &true; en cas de succ&egrave;s
    et &false; sinon.
   </para>
  </refsect1>
 </refentry>

