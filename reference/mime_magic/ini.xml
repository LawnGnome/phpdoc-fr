<section id="mime-magic.configuration">
 &reftitle.runtime;
 &extension.runtime;
 <para>
 <table>
  &ini.table.title;
  <tgroup cols="4">
   <thead>
    &ini.table.top;
   </thead>
   <tbody>
    <row>
     <entry>mime_magic.debug</entry>
     <entry>"0"</entry>
     <entry>PHP_INI_SYSTEM</entry>
     <entry>Disponible depuis &php; 5.0.0.</entry>
    </row>
    <row>
     <entry>mime_magic.magicfile</entry>
     <entry>"/path/to/php/magic.mime"</entry>
     <entry>PHP_INI_SYSTEM</entry>
     <entry>Disponible depuis &php; 4.3.0.</entry>
    </row>
   </tbody>
  </tgroup>
 </table>
 &ini.php.constants;
</para>

&ini.descriptions.title;

<para>
 <variablelist>
  <varlistentry id="ini.mime-magic.debug">
   <term>
    <parameter>mime_magic.debug</parameter>
    &booleen;
   </term>
   <listitem>
    <para>
     Active / Désactive le débogage.
    </para>
   </listitem>
  </varlistentry>
  <varlistentry id="ini.mime-magic.magicfile">
   <term>
    <parameter>mime_magic.magicfile</parameter>
    &string;
   </term>
   <listitem>
    <para>
     Le chemin vers le fichier magic.mime.
    </para>
   </listitem>
  </varlistentry>
 </variablelist>
</para>
</section>

