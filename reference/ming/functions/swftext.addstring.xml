<!-- splitted from ./fr/functions/ming.xml, last change in rev 1.7 -->
<!-- last change to 'swftext.addstring' in en/ tree in rev 1.24 -->
  <refentry id="function.swftext.addstring">
   <refnamediv>
    <refname>swftext->addstring</refname>
    <refpurpose>Ajoute du texte</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>swftext->addstring</methodname>
      <methodparam><type>string</type><parameter>string</parameter></methodparam>
     </methodsynopsis>
     &warn.experimental.func;
    <para>
     <function>swftext->addstring</function> ajoute le texte <parameter>string</parameter>
     au texte courant, et le dessine. Le stylo est situé sur la ligne de base
     du texte, c'est-à-dire que le texte sera écrit horizontalement.
    </para>
   </refsect1>
  </refentry>

