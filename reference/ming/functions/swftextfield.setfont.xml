<!-- splitted from ./fr/functions/ming.xml, last change in rev 1.7 -->
<!-- last change to 'swftextfield.setfont' in en/ tree in rev 1.24 -->
  <refentry id="function.swftextfield.setfont">
   <refnamediv>
    <refname>swftextfield->setfont</refname>
    <refpurpose>Modifie la police du champs</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>void</type><methodname>swftextfield->setfont</methodname>
      <methodparam><type>string</type><parameter>font</parameter></methodparam>
     </methodsynopsis>
     &warn.experimental.func;
    <para>
     <function>swftextfield->setfont</function> remplace la police courante
     par la police <parameter>font</parameter> (police client?).
    </para>
   </refsect1>
  </refentry>

