<!-- splitted from ./fr/functions/math.xml, last change in rev 1.11 -->
<!-- last change to 'pi' in en/ tree in rev 1.2 -->
 <refentry id="function.pi">
  <refnamediv>
   <refname>pi</refname>
   <refpurpose>Retourne la valeur de pi </refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>float</type><methodname>pi</methodname>
     <void/>
    </methodsynopsis>
   <simpara>
    <function>pi</function> retourne la valeur de pi.
   </simpara>
   <para>
    <informalexample>
     <programlisting role="php">
&lt;?php
  echo pi();
//  3.1415926535898
?&gt;
     </programlisting>
    </informalexample>
   </para>
  </refsect1>
 </refentry>

