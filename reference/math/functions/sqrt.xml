<!-- splitted from ./fr/functions/math.xml, last change in rev 1.11 -->
<!-- last change to 'sqrt' in en/ tree in rev 1.2 -->
 <refentry id="function.sqrt">
  <refnamediv>
   <refname>sqrt</refname>
   <refpurpose>Racine carrée</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>float</type><methodname>sqrt</methodname>
     <methodparam><type>float</type><parameter>arg</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>sqrt</function> retourne la racine carrée de
    <parameter>arg</parameter>.
   </para>
    <para>
     <example>
      <title>Exemple avec <function>sqrt</function></title>
      <programlisting>
<![CDATA[
<?php
  // La précision dépend de votre directive precision
  echo sqrt(9); // 3
  echo sqrt(10); // 3.16227766 ...
?>
]]>
      </programlisting>
     </example>
    </para>
    <para>
     Voir aussi
     <function>pow</function>.
    </para>
  </refsect1>
 </refentry>

