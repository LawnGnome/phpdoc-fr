<!-- splitted from ./fr/functions/cpdf.xml, last change in rev 1.12 -->
<!-- last change to 'cpdf-setdash' in en/ tree in rev 1.2 -->
 <refentry id="function.cpdf-setdash">
  <refnamediv>
   <refname>cpdf_setdash</refname>
   <refpurpose>Fixe le motif de pointillés</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>void</type><methodname>cpdf_setdash</methodname>
     <methodparam><type>int</type><parameter>pdf_document</parameter></methodparam>
     <methodparam><type>float</type><parameter>white</parameter></methodparam>
     <methodparam><type>float</type><parameter>black</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>cpdf_setdash</function> fixe le motif de
    pointillés à <parameter>white</parameter>
    unité de blanc et <parameter>black</parameter>
    unité de noir. Si les deux sont à 0,
    une ligne pleine est affichée.
   </para>
  </refsect1>
 </refentry>

