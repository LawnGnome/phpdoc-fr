<refentry id="function.cpdf-add-outline">
 <refnamediv>
  <refname>cpdf_add_outline</refname>
  <refpurpose>Ajoute un signet à la page courante</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>cpdf_add_outline</methodname>
   <methodparam><type>int</type><parameter>pdf_document</parameter></methodparam>
   <methodparam><type>int</type><parameter>lastoutline</parameter></methodparam>
   <methodparam><type>int</type><parameter>sublevel</parameter></methodparam>
   <methodparam><type>int</type><parameter>open</parameter></methodparam>
   <methodparam><type>int</type><parameter>pagenr</parameter></methodparam>
   <methodparam><type>string</type><parameter>text</parameter></methodparam>
  </methodsynopsis>
  <para>
   <function>cpdf_add_outline</function> ajoute un signet avec
   le texte <parameter>text</parameter> qui pointe sur la page
   courante.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &cpdf.pdf-document.description;
    <varlistentry>
     <term><parameter>lastoutline</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>sublevel</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>open</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>pagenr</parameter></term>
     <listitem>
      <para>
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>text</parameter></term>
     <listitem>
      <para>
       Le texte du signet
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Ajouter une mise en relief</title>
    <programlisting role="php">
<![CDATA[
<?php
$cpdf = cpdf_open(0);
cpdf_page_init($cpdf, 1, 0, 595, 842);
cpdf_add_outline($cpdf, 0, 0, 0, 1, "Page 1");
// ...
// quelques dessins
// ...
cpdf_finalize($cpdf);
header("Content-type: application/pdf");
cpdf_output_buffer($cpdf);
cpdf_close($cpdf);
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
</refentry>

