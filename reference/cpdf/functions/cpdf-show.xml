<!-- splitted from ./fr/functions/cpdf.xml, last change in rev 1.12 -->
<!-- last change to 'cpdf-show' in en/ tree in rev 1.2 -->
 <refentry id="function.cpdf-show">
  <refnamediv>
   <refname>cpdf_show</refname>
   <refpurpose>Imprime un texte &agrave; la position courante.</refpurpose>
  </refnamediv>
  <refsect1>
   <title>Description</title>
    <methodsynopsis>
     <type>void</type><methodname>cpdf_show</methodname>
     <methodparam><type>resource</type><parameter>pdf_document</parameter></methodparam>
     <methodparam><type>string</type><parameter>text</parameter></methodparam>
    </methodsynopsis>
   <para>
    <function>cpdf_show</function> imprime la cha&icirc;ne
    <parameter>text</parameter>, &agrave; la position courante.
   </para>
   <para>
    Voir aussi
    <function>cpdf_text</function>,
    <function>cpdf_begin_text</function> et
    <function>cpdf_end_text</function>.
   </para>
  </refsect1>
 </refentry>

