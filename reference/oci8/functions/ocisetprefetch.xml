<!-- splitted from ./fr/functions/oci8.xml, last change in rev 1.13 -->
<!-- last change to 'ocisetprefetch' in en/ tree in rev 1.23 -->
  <refentry id="function.ocisetprefetch">
   <refnamediv>
    <refname>ocisetprefetch</refname>
    <refpurpose>
     Indique le nombre de lignes qui doivent être pré-lues par
      Oracle
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>bool</type><methodname>ocisetprefetch</methodname>
      <methodparam><type>resource</type><parameter>stmt</parameter></methodparam>
      <methodparam><type>int</type><parameter>rows</parameter></methodparam>
     </methodsynopsis>
    <simpara>
     <function>ocisetprefetch</function> spécifie le nombre des premières lignes
     qui doivent être pré-lues à <parameter>rows</parameter> pour la 
     connexion Oracle <parameter>stmt</parameter>. La valeur par défaut 
     de <parameter>rows</parameter> est 1 ligne.
     </simpara>
    </refsect1>
   </refentry>

