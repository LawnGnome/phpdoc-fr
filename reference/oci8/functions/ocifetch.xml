<!-- splitted from ./fr/functions/oci8.xml, last change in rev 1.4 -->
<!-- last change to 'ocifetch' in en/ tree in rev 1.2 -->
  <refentry id="function.ocifetch">
   <refnamediv>
    <refname>ocifetch</refname>
    <refpurpose>
      Lit la prochaine ligne dans le résultat Oracle
    </refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>bool</type><methodname>ocifetch</methodname>
      <methodparam><type>resource</type><parameter>stmt</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>ocifetch</function> lit la prochaine ligne
     (d'une commande SELECT) et la place dans le buffer interne de résultat.
     &return.success;
    </para>
   </refsect1>
  </refentry>

