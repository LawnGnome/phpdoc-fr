<!-- splitted from ./fr/functions/oci8.xml, last change in rev 1.4 -->
<!-- last change to 'ociexecute' in en/ tree in rev 1.2 -->
  <refentry id="function.ociexecute">
   <refnamediv>
    <refname>ociexecute</refname>
    <refpurpose>Exécute une commande SQL Oracle</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Description</title>
     <methodsynopsis>
      <type>bool</type><methodname>ociexecute</methodname>
      <methodparam><type>resource</type><parameter>stmt</parameter></methodparam>
      <methodparam choice="opt"><type>int</type><parameter>mode</parameter></methodparam>
     </methodsynopsis>
    <para>
     <function>ociexecute</function> exécute une commande
     déjà préparée (voir
     <function>ociparse</function>). L'option <parameter>mode</parameter> vous
     permet de spécifier le mode d'exécution (par défaut,
     il est à <constant>OCI_COMMIT_ON_SUCCESS</constant>). Si vous ne voulez pas que la
     commande soit automatiquement validée, utilisez le mode <constant>OCI_DEFAULT</constant>.
    </para>
    <para>
     &return.success;
    </para>
   </refsect1>
  </refentry>

